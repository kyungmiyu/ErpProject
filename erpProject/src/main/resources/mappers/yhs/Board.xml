<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oracle.erpProject.BoardMapper">

   <select id="boardTotal" parameterType="Board"  resultType="board">
		SELECT count(*) FROM BOARD b where b.emp_no=#{emp_no}
   </select>

   <select id="yhsBoardListAll" parameterType="Board" resultType="board">
    <![CDATA[
	select * from board		
]]>
   </select>

</mapper>